    "#neural network\n",
    "# parceptron\n",
    "import numpy as np\n",
    "def step_func(x):\n",
    "    return 1 if x >= 0 else 0\n",
    "\n",
    "def perceptron(weights,bias,x):\n",
    "    calculation = np.dot(weights,x)+ bias\n",
    "    return step_func(calculation)"
   ]
